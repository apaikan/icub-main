            <param name="network">net_headtorso</param>
        </action>
        <action phase="shutdown" type="detach" level="5">
            <param name="device">torso</param>
            <param name="network">net_headtorso</param>
        </action>
    -->


    <!--
        <action phase="interrupt" type="abort" level="5" />

